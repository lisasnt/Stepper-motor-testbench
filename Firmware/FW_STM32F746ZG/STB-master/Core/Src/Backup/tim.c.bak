    // HAL_TIM_OC_Start_IT(&TIMER_BRAKE_STEP_CONTROL, TIMER_BRAKE_STEP_CONTROL_CHANNEL);
    uint32_t volatile ticks = TIM_MS_TO_TICKS(&TIMER_BRAKE_STEP_CONTROL, time);
